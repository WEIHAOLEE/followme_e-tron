<!-- LIGHTS -->

<effect>
 <inherits-from>front-light</inherits-from>
 <object-name>Cone</object-name>
</effect>


<animation>
 <type>select</type>
 <object-name>Cone</object-name>
 <object-name>Cone-texture</object-name>
 <condition>
     <and>
         <not>
             <equals>
                 <property>sim/rendering/shaders/skydome</property>
                 <value>1</value>
             </equals>
         </not>
         <equals>
             <property>sim/multiplay/generic/int[3]</property>
             <value>1</value>
         </equals>
     </and>
 </condition>
</animation>

