  <red>1.0</red>
  <green>0.8</green>
  <blue>0.2</blue>
  <factor-prop>sim/multiplay/generic/int[0]</factor-prop>
 </emission>
</animation>

<animation>
 <type>material</type>
 <object-name>indicater_right</object-name>
